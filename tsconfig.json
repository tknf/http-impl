{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictBindCallApply": true,
    "downlevelIteration": true,
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "target": "es5",
    "baseUrl": ".",
    "noEmitOnError": true,
    "paths": {
      "@tknf/http-impl/_runtime": ["./_runtime/index.ts"],
      "@tknf/http-impl/cloudflare": ["./cloudflare/index.ts"],
      "@tknf/http-impl/deno": ["./deno/index.ts"],
      "@tknf/http-impl/netlify": ["./netlify/index.ts"],
      "@tknf/http-impl/node": ["./node/index.ts"],
      "@tknf/http-impl/vercel": ["./vercel/index.ts"]
    },
    "incremental": true
  },
  "exclude": ["./**/dist", "examples"],
  "watchOptions": {
    "watchFile": "useFsEvents",
    "watchDirectory": "useFsEvents",
    "fallbackPolling": "dynamicPriority"
  }
}
