{
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "lib": ["ES2020", "dom"],
    "module": "ES2020",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictBindCallApply": true,
    "downlevelIteration": true,
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "skipLibCheck": true,
    "target": "ES2020",
    "baseUrl": ".",
    "noEmitOnError": true,
    "paths": {
      "@tknf/http-impl/_runtime": ["./_runtime/index.ts"],
      "@tknf/http-impl/cloudflare": ["./cloudflare/index.ts"],
      "@tknf/http-impl/node": ["./node/index.ts"],
      "@tknf/http-impl/node-connect": ["./node-connect/index.ts"],
      "@tknf/http-impl/node-express": ["./node-express/index.ts"],
      "@tknf/http-impl/netlify": ["./netlify/index.ts"],
      "@tknf/http-impl/vercel": ["./vercel/index.ts"]
    }
  },
  "exclude": ["./**/dist", "examples"],
  "watchOptions": {
    "watchFile": "useFsEvents",
    "watchDirectory": "useFsEvents",
    "fallbackPolling": "dynamicPriority"
  }
}
